,yFAAH,CAF4B,E,aAK5B,YAAA,CAAkE,KAAlE,EAAkE;AAA7D,EAAA,KAAK,EAAC,cAAuD;AAAxC,EAAA,GAA8B,EAA9B,UAAwC;AAAT,EAAA,GAAG,EAAC;AAAK,CAAlE,CAL4B,CAA9B,E;;AAAA,C;;;sDAfA,YAAA,CAcM,KAdN,EAAA,UAAA,EAcM,CAbJ,YAAA,CAYM,KAZN,EAAA,UAAA,EAYM,CAXJ,YAAA,CAUM,KAVN,EAAA,UAAA,EAUM,CATJ,YAAA,CAAwF,KAAxF,EAAwF;AAAnF,IAAA,KAAK,EAAC,WAA6E;AAAhE,IAAA,KAAK,EAAA;AAAA,MAAA,SAAA,EAAe,QAAA,CAAA;AAAf,KAA2D;AAAnC,IAAA,GAAyB,EAAzB,UAAmC;AAAT,IAAA,GAAG,EAAC;AAAK,GAAxF,E,IAAA,E;;AAAA,GASI,EARJ,YAAA,CAAoE,KAApE,EAAoE;AAA9D,IAAA,KAAK,EAAE,QAAA,CAAA,WAAuD;AAA1C,IAAA,GAAgC,EAAhC,UAA0C;AAAT,IAAA,GAAG,EAAC;AAAK,GAApE,E,IAAA,E;;AAAA,GAQI,EAPJ,YAAA,CAMM,KANN,EAAA,UAAA,EAMM,E,kBALJ,YAAA,CAIM,SAJN,EAIM,IAJN,EAIM,WAAA,CAJa,KAAA,CAAA,IAIb,EAJiB,UAAX,GAAW,EAAR;yBAAf,YAAA,CAIM,KAJN,EAIM;AAJoB,MAAA,GAAG,EAAE,GAAG,CAAC,EAI7B;AAJiC,MAAA,KAAK,EAAC;AAIvC,KAJN,E,CACW,GAAG,CAAC,K,kBAAb,YAAA,CAAkI,GAAlI,EAAkI;YAAA;AAA9G,MAAA,KAAK,EAAC,MAAwG;AAAhG,MAAA,KAAK,EAAA;AAAA,QAAA,SAAA,EAAe,QAAA,CAAA,WAAA,CAAY,GAAG,CAAC,EAAJ,GAAM,CAAlB,CAAf;AAAiC,QAAA,OAAA,EAAgB,QAAA,CAAA,OAAA,CAAQ,GAAG,CAAC,EAAJ,GAAM,CAAd;AAAjD;AAA2F,KAAlI,E,iBAAkH,GAAG,CAAC,K,CAAtH,EAA2H;AAAA;AAA3H,K,uCACS,GAAG,CAAC,K,kBAAb,YAAA,CAAkI,GAAlI,EAAkI;YAAA;AAA9G,MAAA,KAAK,EAAC,MAAwG;AAAhG,MAAA,KAAK,EAAA;AAAA,QAAA,SAAA,EAAe,QAAA,CAAA,WAAA,CAAY,GAAG,CAAC,EAAJ,GAAM,CAAlB,CAAf;AAAiC,QAAA,OAAA,EAAgB,QAAA,CAAA,OAAA,CAAQ,GAAG,CAAC,EAAJ,GAAM,CAAd;AAAjD;AAA2F,KAAlI,E,iBAAkH,GAAG,CAAC,K,CAAtH,EAA2H;AAAA;AAA3H,K,uCACS,GAAG,CAAC,K,kBAAb,YAAA,CAAmI,GAAnI,EAAmI;YAAA;AAA/G,MAAA,KAAK,EAAC,OAAyG;AAAhG,MAAA,KAAK,EAAA;AAAA,QAAA,SAAA,EAAe,QAAA,CAAA,WAAA,CAAY,GAAG,CAAC,EAAJ,GAAM,CAAlB,CAAf;AAAiC,QAAA,OAAA,EAAgB,QAAA,CAAA,OAAA,CAAQ,GAAG,CAAC,EAAJ,GAAM,CAAd;AAAjD;AAA2F,KAAnI,E,iBAAmH,GAAG,CAAC,K,CAAvH,EAA4H;AAAA;AAA5H,K,sCAHF,C;GAIM,CAJN,E;;AAAA,GAKI,EANN,CAOI,CAVN,CAWI,CAZN,CAaI,CAdN,C,EAeA,U","sourcesContent":["<template>\n  <div class=\"main-container\">\n    <div class=\"main\">\n      <div class=\"friends-contain\">\n        <img class=\"ryan-main\" :style=\"{ transform: scale }\" src=\"../assets/ryan1.png\" alt=\"\" />\n        <img :class=\"friendsHide\" src=\"../assets/kakaofriends.png\" alt=\"\" />\n        <div class=\"text-wrap\">\n          <div v-for=\"msg in msgs\" :key=\"msg.id\" class=\"text-section\">\n            <p v-if=\"msg.text1\" class=\"desc\" :style=\"{ transform: translation[msg.id - 1], opacity: opacity[msg.id - 1] }\">{{ msg.text1 }}</p>\n            <p v-if=\"msg.text2\" class=\"desc\" :style=\"{ transform: translation[msg.id - 1], opacity: opacity[msg.id - 1] }\">{{ msg.text2 }}</p>\n            <p v-if=\"msg.text3\" class=\"desc2\" :style=\"{ transform: translation[msg.id - 1], opacity: opacity[msg.id - 1] }\">{{ msg.text3 }}</p>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n  <div class=\"content-container\">\n    <h3>Brand Overview</h3>\n    <p>\n      Kakao Friends made of emoticon Characters on kakao talk used in South Korea, normally\n    </p>\n    <img class=\"content-card\" src=\"../assets/card-image.png\" alt=\"\" />\n  </div>\n</template>\n\n<script>\nconst msgs = [\n  {\n    id: 1,\n    text1: '누구에게나',\n    text2: '사랑받는',\n  },\n  {\n    id: 2,\n    text1: '국민캐릭터',\n    text2: '카카오프렌즈',\n  },\n  {\n    id: 3,\n    text1: '즐거운 일상',\n  },\n  {\n    id: 4,\n    text1: '일상의 즐거움',\n  },\n  {\n    id: 5,\n    text3: 'KAKAO FRIENDS',\n  },\n];\n\nexport default {\n  name: 'Contain',\n\n  data() {\n    return {\n      msgs,\n      vt: document.documentElement.clientHeight,\n      scrollTimeout: null,\n      offsetX: 0,\n      offsetY: 0,\n    };\n  },\n  methods: {\n    handleScroll() {\n      if (this.scrollTimeout) {\n        window.cancelAnimationFrame(this.scrollTimeout);\n        // console.log('cancelAnimationFrame');\n      }\n\n      this.scrollTimeout = window.requestAnimationFrame(() => {\n        this.offsetX = window.pageXOffset;\n        this.offsetY = window.pageYOffset;\n        console.log('offsetY : ', this.offsetY, 'requestAnimationFrame');\n        // console.log('this.translation : ', this.translation);\n        // console.log('vt : ', this.vt);\n      });\n    },\n  },\n\n  created() {\n    window.addEventListener('scroll', this.handleScroll, false);\n  },\n  beforeDestroyed() {\n    window.removeEventListener('scroll', this.handleScroll, false);\n  },\n  computed: {\n    translation() {\n      const translate = (this.offsetY / this.vt) * 200;\n      let first, second, third, fourth, fifth;\n      let position = [first, second, third, fourth, fifth];\n      for (let i = 0; i < position.length; i++) {\n        if (this.offsetY > this.vt * i && this.offsetY <= this.vt * (i + 1)) {\n          position[i] = `translateY(${100 - (translate - i * 200)}%)`;\n        } else if (this.offsetY <= this.vt * i) {\n          position[i] = `translateY(100%)`;\n        } else {\n          position[i] = `translateY(-100%)`;\n        }\n      }\n      return position;\n    },\n    opacity() {\n      const scroll = this.offsetY / this.vt;\n      let first, second, third, fourth, fifth;\n      //   for (let i = 0; i < opacity.length; i++) {\n      //     if (this.offsetY > this.vt * i && this.offsetY < this.vt * (i + 1)) {\n      //       opacity[i] = scroll < i + 0.5 ? 0 + (scroll - i) * 2 : 1 - (scroll * 2 - (i * 2 + 1));\n      //     } else {\n      //       opacity[i] = 0;\n      //     }\n      //   }\n\n      if (this.offsetY > this.vt * 0 && this.offsetY <= this.vt * 1) {\n        first = scroll < 0.5 ? 1 : 1 - (scroll * 2 - 1);\n      } else if (this.offsetY === this.vt * 0) {\n        first = 1;\n      } else {\n        first = 0;\n      }\n      if (this.offsetY > this.vt * 1 && this.offsetY <= this.vt * 2) {\n        second = scroll < 1.5 ? 0 + (scroll - 1) * 2 : 1 - (scroll * 2 - 3);\n      } else {\n        second = 0;\n      }\n      if (this.offsetY > this.vt * 2 && this.offsetY <= this.vt * 3) {\n        third = scroll < 2.5 ? 0 + (scroll - 2) * 2 : 1 - (scroll * 2 - 5);\n      } else {\n        third = 0;\n      }\n      if (this.offsetY > this.vt * 3 && this.offsetY <= this.vt * 4) {\n        fourth = scroll < 3.5 ? 0 + (scroll - 3) * 2 : 1 - (scroll * 2 - 7);\n      } else {\n        fourth = 0;\n      }\n      if (this.offsetY > this.vt * 4 && this.offsetY <= this.vt * 5) {\n        fifth = scroll < 4.5 ? 0 + (scroll - 4) * 2 : 1;\n      } else if (this.offsetY > this.vt * 5) {\n        fifth = 1;\n      } else {\n        fifth = 0;\n      }\n      let opacity = [first, second, third, fourth, fifth];\n\n      return opacity;\n    },\n    scale() {\n      //   const scroll = this.offsetY / this.vt;\n      let scaleInt = 15;\n      let translateInt = 0;\n      if (this.offsetY >= this.vt * 0 && this.offsetY < this.vt * 4) {\n        scaleInt = 15 - (this.offsetY / (this.vt * 4)) * 14;\n        translateInt = 0 + (this.offsetY / (this.vt * 4)) * 14;\n      } else {\n        scaleInt = 1;\n        translateInt = 14;\n      }\n\n      //   const scale = `scale(${scaleInt}) translateY(30%)`;\n      //   console.log('scale: ', scale);\n      return `scale(${scaleInt}) translateY(${translateInt + 27}% )`;\n    },\n    friendsHide() {\n      let friendsHide;\n      if (this.offsetY < this.vt * 4.3) {\n        friendsHide = 'kakaofriends-main friends-hide';\n      } else {\n        friendsHide = 'kakaofriends-main';\n      }\n\n      return friendsHide;\n    },\n  },\n};\n</script>\n\n<style>\n.main-container {\n  width: 100%;\n  height: auto;\n  position: relative;\n}\n.main {\n  width: 100%;\n  height: 600vh;\n}\n.text-section {\n  width: 100%;\n  height: 100vh;\n}\n\n.friends-contain {\n  position: sticky;\n  top: 0;\n  width: 100%;\n  height: 100vh;\n  padding: 0;\n  text-align: center;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  overflow: hidden;\n}\n.ryan-main {\n  position: relative;\n  width: 100%;\n  height: auto;\n  z-index: 2;\n}\n.kakaofriends-main {\n  position: absolute;\n  transform: translateY(32%) scale(1);\n  width: 100%;\n  height: auto;\n  transition: 0.2s ease-in;\n  opacity: 1;\n}\n.friends-hide {\n  opacity: 0;\n  transform: translateY(38%) scale(0.7);\n}\n.text-wrap {\n  position: absolute;\n  top: 0;\n  width: 100%;\n  height: 100vh;\n  z-index: 3;\n}\n.text-section {\n  position: absolute;\n  top: 0;\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  align-items: center;\n  padding-bottom: 30vh;\n}\n.desc {\n  text-align: center;\n  font-size: 40px;\n  font-weight: 700;\n}\n.desc2 {\n  text-align: center;\n  color: white;\n  background: #000;\n  padding: 10px 16px 6px 16px;\n  border-radius: 28px;\n\n  font-size: 34px;\n  font-weight: 700;\n}\n\n.content-container {\n  margin: 60px 0;\n  padding: 20px;\n}\n.content-container > h3 {\n  font-size: 36px;\n  font-weight: 700;\n  margin-bottom: 32px;\n}\n.content-container > p {\n  line-height: 1.3;\n  font-size: 22px;\n  margin-bottom: 48px;\n}\n.content-card {\n  width: 100%;\n}\n</style>\n"],"sourceRoot":""}]}